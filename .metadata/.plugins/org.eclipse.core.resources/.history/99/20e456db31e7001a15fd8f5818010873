#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int numeroUno;
	int resultadoScan;

	printf("Ingrese el primer numero");
	resultadoScan = scanf("%d",&numeroUno);

	return EXIT_SUCESS;
}
